<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Frame" id="ib_gui" named="True">
    <property name="background">grey</property>
    <property name="borderwidth">0</property>
    <property name="height">600</property>
    <property name="width">600</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <containerlayout manager="grid">
      <property name="propagate">False</property>
    </containerlayout>
    <child>
      <object class="tk.LabelFrame" id="user_info_container" named="True">
        <property name="background">grey</property>
        <property name="font">{Verdana} 8 {}</property>
        <property name="height">50</property>
        <property name="text" translatable="yes">Info</property>
        <property name="width">580</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
        <containerlayout manager="grid">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Frame" id="fr_user_info" named="True">
            <property name="background">grey</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="row">0</property>
              <property name="sticky">n</property>
            </layout>
            <child>
              <object class="tk.Label" id="lb_user_id" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">ID:</property>
                <layout manager="pack">
                  <property name="anchor">center</property>
                  <property name="expand">false</property>
                  <property name="padx">10</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lb_user_name" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">Name:</property>
                <layout manager="pack">
                  <property name="anchor">center</property>
                  <property name="expand">false</property>
                  <property name="padx">80</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lb_user_address" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">Address:</property>
                <layout manager="pack">
                  <property name="anchor">center</property>
                  <property name="expand">false</property>
                  <property name="padx">50</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lb_user_dob" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">DOB:</property>
                <layout manager="pack">
                  <property name="anchor">center</property>
                  <property name="expand">false</property>
                  <property name="padx">60</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.LabelFrame" id="carrier_container" named="True">
        <property name="background">grey</property>
        <property name="font">{Verdana} 8 {}</property>
        <property name="height">150</property>
        <property name="text" translatable="yes">Carrier</property>
        <property name="width">500</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">n</property>
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Frame" id="fr_carrier_info" named="True">
            <property name="background">grey</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="row">0</property>
            </layout>
            <containerlayout manager="grid">
              <property name="anchor">center</property>
            </containerlayout>
            <child>
              <object class="tk.Label" id="lb_carrier_name" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">center</property>
                <property name="text" translatable="yes">Name</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">50</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="label1" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">center</property>
                <property name="text" translatable="yes">Address</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">50</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="label2" named="True">
                <property name="background">grey</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">center</property>
                <property name="text" translatable="yes">Primary</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="padx">50</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="fr_carrier_list" named="True">
            <property name="background">grey</property>
            <property name="height">50</property>
            <property name="width">450</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="tk.Entry" id="entry_carrier_info" named="True">
                <property name="font">{Verdana} 8 {}</property>
                <property name="justify">center</property>
                <property name="state">readonly</property>
                <property name="text" translatable="yes">w			w			w</property>
                <property name="width">65</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="fr_carrier_remove_btns" named="True">
            <property name="height">15</property>
            <property name="width">15</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="tk.Button" id="btn_remove_carrier" named="True">
                <property name="activeforeground">red</property>
                <property name="command" type="command" cbtype="with_wid">remove_carrier</property>
                <property name="font">{Verdana} 8 {}</property>
                <property name="height">1</property>
                <property name="justify">center</property>
                <property name="text" translatable="yes">x</property>
                <property name="width">1</property>
                <layout manager="pack">
                  <property name="anchor">center</property>
                  <property name="expand">false</property>
                  <property name="ipadx">2</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="fr_gui_control" named="True">
        <property name="background">grey</property>
        <property name="height">50</property>
        <property name="width">500</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">15</property>
          <property name="row">4</property>
        </layout>
        <child>
          <object class="tk.Button" id="btn_back_to_home" named="True">
            <property name="background">#2980b9</property>
            <property name="font">{Verdana} 12 {}</property>
            <property name="foreground">white</property>
            <property name="relief">flat</property>
            <property name="text" translatable="yes">⬅ Home</property>
            <layout manager="grid" />
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btn_save_changes" named="True">
            <property name="background">#2980b9</property>
            <property name="font">{Verdana} 12 {}</property>
            <property name="foreground">white</property>
            <property name="relief">flat</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Save Changes ✓</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">20</property>
              <property name="pady">10</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
